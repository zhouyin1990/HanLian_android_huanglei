<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="hanliankeji.activity.BusineActivity">
    <RelativeLayout
        android:id="@+id/RelativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@mipmap/ic_navbar"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/tv_detail_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="商家注册及维护"
            android:textColor="@color/white"
            android:textSize="22dp" />

        <View
            android:id="@+id/textView11"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:background="@color/line_color" />

        <ImageView
            android:id="@+id/img_busine_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="21dp"
            android:src="@mipmap/ic_back" />

    </RelativeLayout>

    <ImageView
        android:id="@+id/img_b1"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="62dp"
        app:srcCompat="@mipmap/yuan"
        android:layout_below="@+id/RelativeLayout1"
        android:layout_alignLeft="@+id/img_4"
        android:layout_alignStart="@+id/img_4"/>

    <ImageView
        android:id="@+id/img_4"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="88dp"
        app:srcCompat="@mipmap/yuan"
        android:layout_below="@+id/img_b1"
        android:layout_centerHorizontal="true"/>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="注册"
        android:textColor="@color/white"
        android:layout_marginTop="62dp"
        android:layout_alignTop="@+id/img_b1"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"/>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:text="维护"
        android:layout_alignTop="@+id/img_4"
        android:layout_alignRight="@+id/img_4"
        android:layout_alignEnd="@+id/img_4"
        android:layout_marginRight="57dp"
        android:layout_marginEnd="57dp"
        android:layout_marginTop="65dp"/>


</RelativeLayout>
